3px solid pink;\n    overflow-y: hidden;\n    overflow-x: hidden;\n\n    @media only screen and (min-device-width: 1366px) {\n        margin-top: 50px;\n    }\n}\n\n\n.frame {\n    &__child{\n        position: absolute;\n        top: 0;\n        left: 0;\n        right: 0;\n        margin: auto;\n        width: 84%;\n        height: 82%;\n\n        @media only screen and (min-device-width: 1366px) {\n            width: 90%;\n            height: 90vh;\n        }\n    }\n}\n\n.sections-container{\n    height: 100%;\n}\n\n.section{\n    &__bg-image{\n        object-fit: cover;\n        width: 100%;\n        height: 88vh;\n        @media only screen and (min-device-width: 1366px) {\n            width: 100%;\n            height: 100%;\n            margin-top: 0px;\n        }\n    }\n}\n\n\n.mobile{\n    @media only screen and (min-device-width: 1366px) {\n        display: none;\n    }\n}\n\n.desktop{\n    display: none;\n    @media only screen and (min-device-width: 1366px) {\n        display: inherit;\n    }\n}\n\n\n\n.clouds-container{\n    position: absolute;\n    top: 0;\n    left: 0;\n    height: 100%;\n    width: 100%;\n    z-index: 10;\n}\n\n.content{\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    width: 100%;\n    height: 100%;\n    z-index: 11;\n\n    img{\n        width: 80%;\n        @media only screen and (min-device-width: 1366px){\n            width: 50%;\n        }\n    }\n\n    video{\n        width: 100%;\n        height: 100%;\n        position: absolute;\n        z-index: -1;\n    }\n\n    &__description-container{\n        color: white;\n        text-align: center;\n        font-size: 16px;\n        width: 100%;\n    \n        @media only screen and (min-device-width: 1366px){            \n            width: 60%;\n        }\n    \n    }\n\n    &__cta-container{\n\n    }\n\n}\n\n.cta {\n    background