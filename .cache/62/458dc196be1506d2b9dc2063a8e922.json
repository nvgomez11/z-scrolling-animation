  &__bg-image{\n        object-fit: cover;\n        width: 100%;\n        height: 88vh;\n        @media only screen and (min-device-width: 1366px) {\n            width: 100%;\n            height: 100%;\n            margin-top: 0px;\n        }\n    }\n}\n\n\n.mobile{\n    @media only screen and (min-device-width: 1366px) {\n        display: none;\n    }\n}\n\n.desktop{\n    display: none;\n    @media only screen and (min-device-width: 1366px) {\n        display: inherit;\n    }\n}\n\n\n\n.clouds-container{\n    position: absolute;\n    top: 0;\n    left: 0;\n    height: 100%;\n    width: 100%;\n    z-index: 10;\n}\n\n.content{\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    width: 100%;\n    height: 100%;\n    z-index: 11;\n\n    img{\n        width: 80%;\n        @media only screen and (min-device-width: 1366px){\n            width: 50%;\n        }\n    }\n\n    video{\n        width: 100%;\n        height: 100%;\n        position: absolute;\n        z-index: -1;\n    }\n\n    &__description-container{\n        color: white;\n        text-align: center;\n        font-size: 16px;\n        width: 100%;\n    \n        @media only screen and (min-device-width: 1366px){            \n            width: 60%;\n        }\n    \n    }\n\n    &__cta-container{\n\n    }\n\n}\n\n.cta {\n    background-color: #fff;\n    border-radius: 10px;\n    border: 1px solid #fff;\n    display: inline-block;\n    cursor: pointer;\n    color: #c9b657;\n    font-family: Arial;\n    font-size: 16px;\n    padding: 10px 26px;\n    text-decoration: none;\n    font-weight: 800;\n    z-index: 15;\n}\n.cta:hover {\n\tbackground-color:#ffffff;\n}\n.cta:active {\n\tposition:relative;\n\ttop:1px;\n}\n\n.trigger{\n    background-color: red;\n    opacity: 0.5;\n    position: absolute;\n    width: 100px;\n    height: 20vh;\n}\n\n\n.trigger1-anchor{\n    height: 50vh;\n    width: 100px;\n}\n\n.cloud{\n    position: fixed;\n    height: 200px;\n    transform: perspective(300px) translateZ(0px);\n    z-index: 10;\n    @media only screen and (min-device-width: 1366px){\n      height: 350px;\n    }\n}\n\n.topLeftCloud{\n    top: -91px;\n    left: -351px;\n\n    @media only screen and (min-device-width: 1366px){\n        top: -170px;\n        left: -435px;\n    } \n}\n  \n.bottomLeftCloud{\n    left: -467px;\n    bottom: -65px;\n\n    @media only screen and (min-device-width: 1366px){\n        bottom: -111px;\n        left: -520px;\n    }\n  }\n  \n.topRightCloud{\n    top: -66px;\n    right: -470px;\n    @media only screen and (min-device-width: 1366px){\n        top: -222px;\n        right: -367px;\n    }\n  \n  }\n  \n.middleRightCloud{\n    top: -97px;\n    right: -371px;\n    @media only screen and (min-device-width: 1366px){\n        top: -97px;\n        right: -603px;\n    }\n\n  }\n  \n.bottomRightCloud{\n    bottom: -83px;\n    right: -937px;\n    @media only screen and (min-device-width: 1366px){\n        bottom: -100px;\n        right: -1313px;\n    }\n}\n\n.crane{\n    display: none;\n    height: 115px;\n    position: absolute;\n    z-index: 11;\n\n    @media only screen and (min-device-width: 1366px){\n        display: unset;\n        height: 200px;\n    }\n}\n\n.craneTopLeft{\n    top: -59px;\n    left: 8px;\n\n    @media only screen and (min-device-width: 1366px){\n        top: -70px;\n        left: 212px;\n    }\n}\n\n.craneBottomLeft{\n    top: 663px;\n    left: 203px;\n}\n\n\n.indicators-container{\n    position: absolute;\n    left: 0;\n    top: 45vh;\n    transform: translateY(-50%);\n    width: 7vw;\n    background-color: white;\n    border: 2px solid white;\n    border-radius: 25px;\n    z-index: 4;\n\n    @media only screen and (min-device-width: 1366px) {        \n        width: 4vw;    \n        margin-top: 0px;        \n    }\n\n    &__controls{\n        display: flex;\n        flex-direction: column;\n        justify-content: center;\n        align-items: center;\n        padding-left: 0px;\n        list-style-type: none;\n        div {\n            width: 10px;\n            height: 10px;\n            border-radius: 20px;\n            margin: 15px 0px 15px 0px;\n            background-color: rgb(139,112,67);\n        }\n    }\n}\n\n.icons-container{\n    position: absolute;\n    bottom: 20px;\n    left: 50%;\n    transform: translateX(-50%);\n    width: 180px;\n    background-color: white;\n    border: 2px solid white;\n    border-radius: 25px;\n    z-index: 4;\n\n    @media only screen and (min-device-width: 1366px) {   \n        height: fit-content;\n        left: unset;     \n        top: 50%;\n        transform: translateY(-50%);\n        right: 0;\n        width: 4vw;\n        margin-top: 0px;        \n    }\n    \n\n    &__controls{\n        display: flex;\n        flex-direction: row;\n        justify-content: space-evenly;\n        align-items: center;\n        padding-left: 0px;\n        list-style-type: none;\n        \n        @media only screen and (min-device-width: 1366px) {  \n            flex-direction: column;      \n            justify-content: center;\n        }\n\n        a {\n            \n            button {\n                height: 10px;\n                border-radius: 30px;\n            }\n        }\n    }\n}\n\n"},"lineCount":null}},"error":null,"hash":"d8abc5ee30eea409900c39cffebea95f","cacheData":{"env":{}}}